<%- include("header") -%>
<div class="box" id="heading">
  <h1><%= typeOfList %></h1>
</div>

<div class="box">
  <% for(let i = 0; i < newItems.length; i++){ %>
  <div class="item">
    <input type="checkbox">
    <p><%= newItems[i] %></p>
  </div>
  <% } %>

  <form action="\" method="post">
    <input type="text" name="item" placeholder="New Item" autocomplete="off">
    <button type="submit" name="submit" value=<%= typeOfList %>>+</button>
  </form>
</div>

<%- include("footer") -%>